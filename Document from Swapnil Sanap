    {
      "cell_type": "code",
      "source": [
        "# McCulloch-Pitts Neuron for NAND Gate\n",
        "\n",
        "import numpy as np\n",
        "\n",
        "# Define all possible input combinations (A, B)\n",
        "inputs = np.array([\n",
        "    [0, 0],\n",
        "    [0, 1],\n",
        "    [1, 0],\n",
        "    [1, 1]\n",
        "])\n",
        "\n",
        "# Define weights and threshold\n",
        "# NAND = NOT(AND), so we use negative weights\n",
        "weights = np.array([-1, -1])\n",
        "threshold = -1   # neuron fires if both inputs are not 1\n",
        "\n",
        "# Define the McCulloch-Pitts neuron model\n",
        "def mcp_neuron(x, w, theta):\n",
        "    net = np.dot(x, w)  # net input = x1*w1 + x2*w2\n",
        "    if net >= theta:\n",
        "        return 1\n",
        "    else:\n",
        "        return 0\n",
        "\n",
        "# Display the results\n",
        "print(\"NAND Gate using McCulloch-Pitts Neuron\\n\")\n",
        "print(\"A  B  |  Output (A NAND B)\")\n",
        "print(\"---------------------------\")\n",
        "for x in inputs:\n",
        "    y = mcp_neuron(x, weights, threshold)\n",
        "    print(f\"{x[0]}  {x[1]}  |      {y}\")\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
