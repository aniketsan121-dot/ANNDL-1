        "#Sigmoid function\n",
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "#sigmoid\n",
        "x=np.arange(-10,10)\n",
        "y=[]\n",
        "for i in range (x.shape[0]):\n",
        "  y.append((1/(1+np.exp(-x[i]))))\n",
        "plt.plot(x,y,c=\"red\")\n",
        "plt.title(\"sigmoid\")\n",
        "plt.grid()\n",
        "plt.show()"
