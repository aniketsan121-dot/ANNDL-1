      "cell_type": "code",
      "source": [
        "# binary step fun\n",
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "# Binary activation function\n",
        "x = np.arange(-2, 2, 0.1)   # finer range for smoother plot\n",
        "y = []\n",
        "\n",
        "for i in range(x.shape[0]):\n",
        "    if x[i] > 0:\n",
        "        y.append(1)   # output 1 for positive input\n",
        "    else:\n",
        "        y.append(0)   # output 0 for zero or negative input\n",
        "\n",
        "plt.plot(x, y, c=\"red\")\n",
        "plt.title(\"Binary Step Function\")\n",
        "plt.grid()\n",
        "plt.show()\n"
      ],
