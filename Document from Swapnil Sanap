        "# tanh activation\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Tanh activation function\n",
        "x = np.arange(-10, 10, 0.1)      # Input values from -10 to 10\n",
        "y = np.tanh(x)                   # Apply tanh activation to each x value\n",
        "\n",
        "# Plotting\n",
        "plt.plot(x, y, c='purple')       # Plot tanh curve in purple color\n",
        "plt.title(\"Tanh Activation Function\")\n",
        "plt.xlabel(\"Input (x)\")\n",
        "plt.ylabel(\"Output f(x)\")\n",
        "plt.grid(True)\n",
        "plt.axhline(0, color='black')    # Add x-axis\n",
        "plt.axvline(0, color='black')    # Add y-axis\n",
        "plt.show()\n"
