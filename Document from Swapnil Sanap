      "cell_type": "code",
      "source": [
        "# bipolar activation\n",
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "# Bipolar activation function: y = 1 if x > 0, else y = -1\n",
        "x = np.arange(-10, 10)\n",
        "y = []\n",
        "\n",
        "for i in range(x.shape[0]):\n",
        "    if x[i] > 0:\n",
        "        y.append(1)\n",
        "    else:\n",
        "        y.append(-1)\n",
        "\n",
        "plt.plot(x, y, c=\"red\")\n",
        "plt.title(\"Bipolar Activation Function\")\n",
        "plt.grid()\n",
        "plt.hlines(y=0, xmin=-10, xmax=9, colors='black')  # horizontal axis line\n",
        "plt.show()\n"
      ],
