    {
      "cell_type": "code",
      "source": [
        "# step activation\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Step Activation Function\n",
        "x = np.arange(-10, 10, 0.1)     # Input values from -10 to +10\n",
        "y = []                          # Empty list to store outputs\n",
        "\n",
        "for i in range(x.shape[0]):\n",
        "    if x[i] >= 0:\n",
        "        y.append(1)             # If x >= 0  y = 1\n",
        "    else:\n",
        "        y.append(0)             # If x < 0  y = 0\n",
        "\n",
        "# Plot the Step Function\n",
        "plt.plot(x, y, c='blue')\n",
        "plt.title(\"Step Activation Function\")\n",
        "plt.xlabel(\"Input (x)\")\n",
        "plt.ylabel(\"Output f(x)\")\n",
        "plt.grid(True)\n",
        "plt.axhline(0, color='black')   # x-axis\n",
        "plt.axvline(0, color='black')   # y-axis\n",
        "plt.show()\n"
      ],
